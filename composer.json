{
  "name": "stonedonsteel.org/website",
  "type": "project",
  "homepage": "https://example.org/",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Fabian Michael",
      "email": "fabian@bessermitsenf.de",
      "homepage": "https://bessermitsenf.de"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.bessermitsenf.de/"
    }
  ],
  "require": {
    "bessermitsenf/kirby-seedling": "dev-main"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "4.0.x-dev"
  },
  "scripts": {
    "linter": "vendor/bin/php-cs-fixer fix --dry-run --diff",
    "linter:fix": "vendor/bin/php-cs-fixer fix --diff"
  },
  "config": {
      "optimize-autoloader": true,
      "allow-plugins": {
          "getkirby/composer-installer": true
      }
  }
}
